.cart-info {
  width: 100%;
  padding-left: 30rpx;
  padding-right: 30rpx;
  padding-top: 30rpx;
}
.cart-list {
  width: 100%;
  padding: 0 20rpx;
  margin-bottom: 20rpx;
  background-color: #ffffff;
  border-radius: 20rpx;
}
.cart-list .shop-info {
  display: flex;
  flex-direction: row;
  width: 100%;
  height: 100rpx;
}
.cart-list .shop-info .check {
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-right: 20rpx;
}
.cart-list .shop-info .check .image {
  width: 38rpx;
  height: 38rpx;
}
.cart-list .shop-info .shop-data {
  display: flex;
  flex-direction: row;
  align-items: center;
}
.cart-list .shop-info .shop-data .image {
  width: 38rpx;
  height: 38rpx;
  margin-right: 10rpx;
}
.cart-list .shop-info .shop-data .text {
  font-size: 28rpx;
  color: #333333;
}
.cart-list .cart-goods {
  width: 100%;
}
.cart-list .cart-goods .goods-item {
  display: flex;
  flex-direction: row;
  align-items: center;
  width: 100%;
  height: 140rpx;
  margin-bottom: 20rpx;
}
.cart-list .cart-goods .goods-item .check {
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-right: 20rpx;
}
.cart-list .cart-goods .goods-item .check .image {
  width: 38rpx;
  height: 38rpx;
}
.cart-list .cart-goods .goods-item .goods {
  display: flex;
  flex-direction: row;
  flex: 1;
  height: 100%;
}
.cart-list .cart-goods .goods-item .goods .pic {
  display: flex;
  align-items: center;
  margin-right: 20rpx;
}
.cart-list .cart-goods .goods-item .goods .pic .image {
  width: 140rpx;
  height: 140rpx;
  border-radius: 10rpx;
}
.cart-list .cart-goods .goods-item .goods .info {
  flex: 1;
  height: 100%;
}
.cart-list .cart-goods .goods-item .goods .info .title {
  display: flex;
  align-items: center;
}
.cart-list .cart-goods .goods-item .goods .info .title .text {
  width: 100%;
  height: 50rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-size: 28rpx;
  color: #333333;
}
.cart-list .cart-goods .goods-item .goods .info .attr {
  display: flex;
  flex-direction: row;
  align-items: center;
}
.cart-list .cart-goods .goods-item .goods .info .attr .text {
  width: 100%;
  height: 40rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-size: 24rpx;
  color: #aaaaaa;
}
.cart-list .cart-goods .goods-item .goods .info .price-num {
  display: flex;
  flex-direction: row;
  align-items: flex-end;
  justify-content: space-between;
  width: 100%;
  height: 50rpx;
}
.cart-list .cart-goods .goods-item .goods .info .price-num .price {
  display: flex;
  flex-direction: row;
  align-items: flex-end;
}
.cart-list .cart-goods .goods-item .goods .info .price-num .price .min {
  font-size: 20rpx;
  color: #fe9a5c;
}
.cart-list .cart-goods .goods-item .goods .info .price-num .price .max {
  font-size: 30rpx;
  font-weight: bold;
  color: #fe9a5c;
}
.cart-list .cart-goods .goods-item .goods .info .price-num .goods-num {
  display: flex;
  flex-direction: row;
  align-items: center;
}
.cart-list .cart-goods .goods-item .goods .info .price-num .goods-num .add-sub {
  width: 38rpx;
  height: 38rpx;
}
.cart-list .cart-goods .goods-item .goods .info .price-num .goods-num .nums {
  width: 80rpx;
  text-align: center;
  font-size: 24rpx;
  font-weight: bold;
  color: #333333;
}
/*底部合计*/
.footer-total {
  position: fixed;
  bottom: 0;
  left: 0;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  width: 100%;
  height: 120rpx;
  padding: 0 30rpx;
  background-color: #ffffff;
}
.footer-total .check-all {
  display: flex;
  flex-direction: row;
  align-items: center;
}
.footer-total .check-all .image {
  width: 38rpx;
  height: 38rpx;
  margin-right: 20rpx;
}
.footer-total .check-all .text {
  font-size: 28rpx;
  color: #333333;
}
.footer-total .total-btn {
  display: flex;
  flex-direction: row;
  align-items: center;
}
.footer-total .total-btn .total {
  display: flex;
  flex-direction: row;
  align-items: center;
}
.footer-total .total-btn .total .tit {
  font-size: 28rpx;
  color: #aaaaaa;
}
.footer-total .total-btn .total .price {
  display: flex;
  flex-direction: row;
  align-items: center;
}
.footer-total .total-btn .total .price .min {
  font-size: 24rpx;
  color: #fe9a5c;
  margin-top: 10rpx;
}
.footer-total .total-btn .total .price .max {
  font-size: 42rpx;
  font-weight: bold;
  color: #fe9a5c;
}
.footer-total .total-btn .btn {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  width: 200rpx;
  height: 70rpx;
  margin-left: 20rpx;
  background-image: linear-gradient(to right, #fb9e6d, #ff6e37);
  border-radius: 35rpx;
}
.footer-total .total-btn .btn .text {
  font-size: 28rpx;
  color: #ffffff;
}
.title-info {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  width: 100%;
  margin-bottom: 20rpx;
}
.title-info .title {
  display: flex;
  flex-direction: row;
  align-items: center;
}
.title-info .title .line {
  width: 10rpx;
  height: 40rpx;
  background-color: #fe9a5c;
  border-radius: 10rpx;
  margin: 0 20rpx;
}
.title-info .title .tit {
  font-size: 28rpx;
  font-weight: bold;
  color: #333333;
}
.lose-efficacy {
  width: 100%;
  padding: 0 30rpx;
  box-sizing: border-box;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}
.lose-efficacy .cart-goods {
  width: 100%;
  background-color: #ffffff;
  border-radius: 20rpx;
}
.lose-efficacy .cart-goods .title-infos {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  width: 100%;
  height: 100rpx;
  padding: 0 30rpx;
}
.lose-efficacy .cart-goods .title-infos .title .text {
  font-size: 26rpx;
  color: #333333;
}
.lose-efficacy .cart-goods .title-infos .del {
  display: flex;
  align-items: center;
}
.lose-efficacy .cart-goods .title-infos .del .text {
  font-size: 26rpx;
  color: #aaaaaa;
}
.lose-efficacy .cart-goods .goods-lists {
  width: 100%;
  padding: 0 30rpx 20rpx;
}
.lose-efficacy .cart-goods .goods-lists .lists {
  display: flex;
  flex-direction: row;
  align-items: center;
  width: 100%;
  margin-bottom: 20rpx;
}
.lose-efficacy .cart-goods .goods-lists .lists .pic {
  margin-right: 20rpx;
}
.lose-efficacy .cart-goods .goods-lists .lists .pic .image {
  width: 140rpx;
  height: 140rpx;
}
.lose-efficacy .cart-goods .goods-lists .lists .info {
  flex: 1;
  height: 140rpx;
}
.lose-efficacy .cart-goods .goods-lists .lists .info .name {
  display: flex;
  flex-direction: row;
  align-items: center;
  width: 100%;
}
.lose-efficacy .cart-goods .goods-lists .lists .info .name .text {
  width: 100%;
  height: 60rpx;
  lines: 2;
  text-overflow: ellipsis;
  font-size: 28rpx;
  color: #aaaaaa;
}
.lose-efficacy .cart-goods .goods-lists .lists .info .tag {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  height: 60rpx;
  margin-top: 20rpx;
  background-color: rgba(254, 154, 92, 0.3);
  border-radius: 10rpx;
}
.lose-efficacy .cart-goods .goods-lists .lists .info .tag .text {
  font-size: 26rpx;
  color: #fe9a5c;
}
/*猜你喜欢*/
.guess-like {
  width: 100%;
  padding: 0 30rpx;
}
.guess-like .goods-list {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-between;
  width: 100%;
}
.guess-like .goods-list .list {
  width: 334rpx;
  margin-bottom: 20rpx;
  padding-bottom: 20rpx;
  background-color: #ffffff;
  border-radius: 20rpx;
}
.guess-like .goods-list .list .pic {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  height: 334rpx;
}
.guess-like .goods-list .list .pic .image {
  width: 334rpx;
  height: 334rpx;
  border-radius: 10rpx 10rpx 0 0;
}
.guess-like .goods-list .list .title {
  display: flex;
  margin-top: 10rpx;
  padding: 0 20rpx;
}
.guess-like .goods-list .list .title .text {
  lines: 2;
  text-overflow: ellipsis;
  width: 334rpx;
  height: 70rpx;
  font-size: 28rpx;
  color: #333333;
}
.guess-like .goods-list .list .price {
  display: flex;
  flex-direction: row;
  align-items: flex-end;
  padding: 0 20rpx;
}
.guess-like .goods-list .list .price .min {
  font-size: 22rpx;
  font-weight: bold;
  margin-bottom: 4rpx;
  color: #fe9a5c;
}
.guess-like .goods-list .list .price .max {
  font-size: 36rpx;
  font-weight: bold;
  color: #fe9a5c;
}
/* 空购物车样式 */
.empty-cart {
  padding: 100rpx 0;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.empty-cart .empty-img {
  width: 300rpx;
  height: 300rpx;
}
.empty-cart .empty-text {
  margin-top: 20rpx;
  font-size: 28rpx;
  color: #999;
}
.empty-cart .go-index {
  margin-top: 40rpx;
  padding: 20rpx 40rpx;
  background-color: #ff5500;
  color: #fff;
  border-radius: 50rpx;
  font-size: 28rpx;
}
/* 底部合计栏（如果 cart.scss 没定义，补一份） */
.footer-total {
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  height: 100rpx;
  padding: 0 24rpx;
  background-color: #ffffff;
  box-shadow: 0 -4rpx 12rpx rgba(0, 0, 0, 0.06);
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}
.footer-total .check-all {
  display: flex;
  flex-direction: row;
  align-items: center;
}
.footer-total .check-all .image {
  width: 36rpx;
  height: 36rpx;
  margin-right: 8rpx;
}
.footer-total .check-all .text {
  font-size: 28rpx;
  color: #333;
}
.footer-total .total-btn {
  display: flex;
  flex-direction: row;
  align-items: center;
}
.footer-total .total-btn .total {
  margin-right: 20rpx;
  display: flex;
  flex-direction: row;
  align-items: baseline;
}
.footer-total .total-btn .total .tit {
  font-size: 26rpx;
  color: #666;
}
.footer-total .total-btn .total .price {
  display: flex;
  flex-direction: row;
  align-items: baseline;
}
.footer-total .total-btn .total .price .min {
  font-size: 24rpx;
  color: #ff5500;
}
.footer-total .total-btn .total .price .max {
  font-size: 32rpx;
  font-weight: 700;
  color: #ff5500;
  margin-left: 2rpx;
}
.footer-total .total-btn .btn {
  padding: 16rpx 40rpx;
  background-color: #ff5500;
  border-radius: 50rpx;
}
.footer-total .total-btn .btn .text {
  font-size: 28rpx;
  color: #ffffff;
}